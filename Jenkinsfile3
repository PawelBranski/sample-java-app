pipeline {
    agent { docker { image 'maven:3.8.4-openjdk-11-slim' } }
    stages {
        stage('build') {
            steps {
                sh 'pwd'
            }
        }
        stage('SonarQube analysis') {
            withSonarQubeEnv(installationName: 'sq1') {
              sh 'ls -al'
            }
       }
    }
}
